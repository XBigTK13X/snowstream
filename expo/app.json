{
  "expo": {
    "version": "0.14.5",
    "scheme": "com.simplepathstudios.snowstream",
    "newArchEnabled": false,
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/icon/android/tvbanner.png",
          "appleTVImages": {
            "icon": "./assets/icon/ios/icon-1280x768.png",
            "iconSmall": "./assets/icon/ios/icon-400x240.png",
            "iconSmall2x": "./assets/icon/ios/icon-800x480.png",
            "topShelf": "./assets/icon/ios/icon-1920x720.png",
            "topShelf2x": "./assets/icon/ios/icon-3840x1440.png",
            "topShelfWide": "./assets/icon/ios/icon-2320x720.png",
            "topShelfWide2x": "./assets/icon/ios/icon-4640x1440.png"
          }
        }
      ],
      "expo-router",
      "expo-secure-store",
      "react-native-video",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ]
    ],
    "splash": {
      "image": "./assets/icon/splash.png",
      "backgroundColor": "#ff9a3f"
    },
    "icon": "./assets/icon/icon.png",
    "name": "snowstream",
    "slug": "snowstream",
    "orientation": "landscape",
    "android": {
      "package": "com.simplepathstudios.snowstream",
      "edgeToEdgeEnabled": true
    },
    "ios": {
      "bundleIdentifier": "com.simplepathstudios.snowstream"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e0fc4a42-5cc7-4ba9-b251-8383cf957527"
      }
    }
  }
}